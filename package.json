{
  "name": "all-animation-css",
  "version": "3.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf css && npm run build-sass && npm run build-autoprefix && npm run build-cleancss",
    "build-sass": "node-sass --output-style expanded scss/main.scss ./dist/all-animation.css",
    "build-autoprefix": "postcss --use autoprefixer --map --output ./dist/all-animation.css ./dist/all-animation.css",
    "build-cleancss": "cleancss -o ./dist/all-animation.min.css ./dist/all-animation.css"
  },
  "files": [
    "scss"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.3.7",
    "clean-css-cli": "^5.4.1",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "rimraf": "^3.0.2"
  }
}
